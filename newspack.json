{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/",
	"preferredVersions": {
		"php": "latest",
		"wp": "latest"
	},
	"features": {
		"networking": true
	},
	"steps": [
		{
			"step": "resetData"
		},
		{
			"step": "installPlugin",
			"pluginData": {
				"resource": "url",
				"url": "https://github.com/Automattic/newspack-plugin/releases/download/v6.32.0/newspack-plugin.zip"
			},
			"options": {
				"activate": true
			}
		},
		{
			"step": "installTheme",
			"themeData": {
				"resource": "url",
				"url": "https://github.com/Automattic/newspack-block-theme/releases/download/v1.24.0/newspack-block-theme.zip"
			},
			"options": {
				"activate": true,
				"importStarterContent": true
			}
		},
		{
			"step": "importWxr",
			"file": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WordPress/theme-test-data/refs/heads/master/themeunittestdata.wordpress.xml"
			}
		},
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"WP_DEBUG": true,
				"WP_DEBUG_DISPLAY": true
			}
		},
		{
			"step": "login",
			"username": "admin"
		}
	]
}
