{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/wp-admin/admin.php?page=newspack-setup-wizard",
	"preferredVersions": {
		"php": "latest",
		"wp": "latest"
	},
	"features": {
		"networking": true
	},
	"plugins": [
		"https://github.com/Automattic/newspack-plugin/releases/download/v6.32.0/newspack-plugin.zip",
		"https://github.com/Automattic/newspack-blocks/releases/download/v4.24.0/newspack-blocks.zip",
		"jetpack",
		"akismet",
		"wordpress-seo"
	],
	"siteOptions": {
		"blogname": "Newspack FSE Sandbox",
		"blogdescription": "A testing ground for the Newspack environment",
		"activation_redirect_timestamp_free": 9999999
	},
	"login": true,
	"steps": [
		{
			"step": "resetData"
		},
        {
            "step": "activatePlugin",
            "pluginName": "Newspack",
            "pluginPath": "/wordpress/wp-content/plugins/newspack-plugin"
        },
		{
			"step": "installTheme",
			"themeData": {
				"resource": "url",
				"url": "https://github.com/Automattic/newspack-block-theme/releases/download/v1.24.0/newspack-block-theme.zip"
			},
			"options": {
				"activate": true,
				"importStarterContent": true
			}
		},
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"WP_DEBUG": true,
				"WP_DEBUG_DISPLAY": true
			}
		}
	]
}
